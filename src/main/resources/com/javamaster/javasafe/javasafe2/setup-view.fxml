<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="222.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javamaster.javasafe.javasafe2.controller.login.SetupViewController">
   <children>
      
      <GridPane layoutX="29.0" layoutY="19.0" prefHeight="182.0" prefWidth="552.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <PasswordField fx:id="masterKey" prefHeight="25.0" prefWidth="237.0" GridPane.columnIndex="1" />
            <Label alignment="CENTER" contentDisplay="CENTER" text="Enter a Master Key to secure the Password list:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label text="Confirm Master Key" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <PasswordField fx:id="confirmMasterKey" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <ButtonBar layoutX="161.0" layoutY="181.0" prefHeight="40.0" prefWidth="276.0" AnchorPane.rightAnchor="163.0">
        <buttons>
          <Button fx:id="setKeyButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onSetMasterKeyClick" text="Set Master Key" textAlignment="CENTER" />
        </buttons>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets right="100.0" />
         </padding>
      </ButtonBar>
   </children>
</AnchorPane>
